{
  "name": "ecom-backend",
  "version": "1.0.0",
  "description": "API Backend pour Ecom - Application de transport et marketplace au Togo",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "build": "prisma generate && prisma db push --accept-data-loss",
    "postinstall": "prisma generate",
    "test": "jest",
    "test:watch": "jest --watch",
    "migrate": "npx prisma migrate dev",
    "generate": "npx prisma generate",
    "seed": "node prisma/seed.js",
    "docker:build": "docker build -t ecom-backend .",
    "docker:run": "docker-compose up -d"
  },
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "axios": "^1.6.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-slow-down": "^2.0.1",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^7.0.10",
    "redis": "^4.6.10",
    "socket.io": "^4.7.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "twilio": "^4.19.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "prisma": "^5.6.0",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "ecom",
    "togo",
    "transport",
    "marketplace",
    "mobile-money",
    "yas",
    "flooz",
    "api"
  ],
  "author": "SKK Analytics",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}
